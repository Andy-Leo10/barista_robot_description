<?xml version="1.0"?>
<robot name="barista">

  <material name="red">
    <color rgba="1.0 0.0 0.0 0.7"/>
  </material>

  <material name="green_light">
    <color rgba="0.0 1.0 0.0 0.7"/>
  </material>

  <material name="green_dark">
    <color rgba="0.0 0.5 0.0 0.7"/>
  </material>

  <material name="blue">
    <color rgba="0.0 0.0 1.0 0.7"/>
  </material>

  <!--base link-->
  <link name="base_link">
  </link>

  <!--chassis cylinder D356 H155-->
  <joint name="base_link_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0 0 0.0775" rpy="0 0 0"/>
  </joint>

  <link name="chassis">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
    </collision>
  </link>

  <!--wheels cylinder R35.2 H20.6-->
  <!--left wheel-->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="chassis"/>
    <child link="left_wheel"/>
    <origin xyz="0 -0.150 -0.0775" rpy="0 0 0"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <link name="left_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </collision>
  </link>
  <!--right wheel-->
  <joint name="right_wheel_joint" type="continuous">
    <parent link="chassis"/>
    <child link="right_wheel"/>
    <origin xyz="0 +0.150 -0.0775" rpy="0 0 0"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <link name="right_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="-1.5707 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="-1.5707 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </collision>
  </link>

  <!--standoff_rods cylinder D20 H220-->
  <!--rod at (xp yp)-->
  <joint name="standoff_rod_xpyp_joint" type="fixed">
    <parent link="chassis"/>
    <child link="standoff_rod_xpyp"/>
    <origin xyz="0.075 0.075 0.1875" rpy="0 0 0"/>
  </joint>

  <link name="standoff_rod_xpyp">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
      <material name="green_dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
    </collision>
  </link>

  <!--rod at (xp yn)-->
  <joint name="standoff_rod_xpyn_joint" type="fixed">
    <parent link="chassis"/>
    <child link="standoff_rod_xpyn"/>
    <origin xyz="0.075 -0.075 0.1875" rpy="0 0 0"/>
  </joint>

  <link name="standoff_rod_xpyn">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
      <material name="green_dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
    </collision>
  </link>

  <!--rod at (xn yp)-->
  <joint name="standoff_rod_xnyp_joint" type="fixed">
    <parent link="chassis"/>
    <child link="standoff_rod_xnyp"/>
    <origin xyz="-0.075 0.075 0.1875" rpy="0 0 0"/>
  </joint>

  <link name="standoff_rod_xnyp">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
      <material name="green_dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
    </collision>
  </link>

  <!--rod at (xn yn)-->
  <joint name="standoff_rod_xnyn_joint" type="fixed">
    <parent link="chassis"/>
    <child link="standoff_rod_xnyn"/>
    <origin xyz="-0.075 -0.075 0.1875" rpy="0 0 0"/>
  </joint>

  <link name="standoff_rod_xnyn">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
      <material name="green_dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.220" radius="0.020"/>
      </geometry>
    </collision>
  </link>

  <!--cup holder tray cylinder D320 H90-->
  <joint name="cup_holder_tray_joint" type="fixed">
    <parent link="chassis"/>
    <child link="cup_holder_tray"/>
    <origin xyz="0 0 0.3425" rpy="0 0 0"/>
  </joint>

  <link name="cup_holder_tray">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.090" radius="0.160"/>
      </geometry>
      <material name="green_light"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.090" radius="0.160"/>
      </geometry>
    </collision>
  </link>



  <!--caster wheels a nested roll-pitch-yaw cylinder R10 H2-cylinder R10 H2-sphere R35.2-->
  <!--front caster wheel-->
  <joint name="front_roll_joint" type="continuous">
    <parent link="chassis"/>
    <child link="front_caster_wheel_r"/>
    <origin xyz="0.100 0 -0.0775" rpy="0 0 0"/>
    <axis xyz="1 0 0" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="front_caster_wheel_r">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_pitch_joint" type="continuous">
    <parent link="front_caster_wheel_r"/>
    <child link="front_caster_wheel_p"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="front_caster_wheel_p">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_yaw_joint" type="continuous">
    <parent link="front_caster_wheel_p"/>
    <child link="front_caster_wheel_y"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="front_caster_wheel_y">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
    </collision>
  </link>
  <!--back caster wheel-->
  <joint name="back_roll_joint" type="continuous">
    <parent link="chassis"/>
    <child link="back_caster_wheel_r"/>
    <origin xyz="-0.100 0 -0.0775" rpy="0 0 0"/>
    <axis xyz="1 0 0" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="back_caster_wheel_r">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
    </collision>
  </link>

  <joint name="back_pitch_joint" type="continuous">
    <parent link="back_caster_wheel_r"/>
    <child link="back_caster_wheel_p"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="back_caster_wheel_p">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.002" radius="0.010"/>
      </geometry>
    </collision>
  </link>

  <joint name="back_yaw_joint" type="continuous">
    <parent link="back_caster_wheel_p"/>
    <child link="back_caster_wheel_y"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>
  <link name="back_caster_wheel_y">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
    </collision>
  </link>

</robot>